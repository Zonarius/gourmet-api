{
  "name": "gourmet-api",
  "version": "0.0.0",
  "description": "gourmet-api",
  "license": "MIT",
  "repository": "",
  "author": "",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/api.js",
  "typings": "lib/api.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "build-only": "tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**.spec.ts",
    "test-only": "mocha --compilers ts:ts-node/register --recursive test/**/*.spec.ts",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'test/**/*.test.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/cheerio": "^0.22.1",
    "@types/mocha": "^2.0.0",
    "@types/node": "^7.0.0",
    "@types/request": "0.0.43",
    "@types/request-promise-native": "^1.0.5",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
